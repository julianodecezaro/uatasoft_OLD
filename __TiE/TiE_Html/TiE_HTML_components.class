    /**
    * Classe que abstrai a inserção de um elemento SELECT do HTML.
    * Data de criação: 28/03/2011
    * Histórico de Manutenções: (Data, Autor, Descrição da Alteração)
    *
    * @author Juliano S. De Cezaro <juliano_cezaro@uniritter.edu.br>
    * @version 1.0.0
    */
    class select extends element
    {
       /**
        * Método construtor da classe select.
        * Data de criação: 28/03/2011
        * Histórico de Manutenções: (Data, Autor, Descrição da Alteração)
        *
        * @author Juliano S. De Cezaro <juliano_cezaro@uniritter.edu.br>
        * @version 1.0.0
        *
        * @param String $_attributes Atributos do elemento.
        * @param Array $_arrValues Array de valores [option].
        * @return void
        */
        public function __construct ($_attributes = "", $_arrValues = "", $_default = "")
        {
			if($_default) {
				$arrValues[""] = $_default;
				foreach($_arrValues as $id=>$value) {
					$arrValues[$id] = $value;
				}
			}else {
				$arrValues = $_arrValues;
			}
            parent::__construct('select', $_attributes);
            $this->id = $this->params->id;
            $this->arrValues = $arrValues;            
        }
       
       /**
        * Método que retorna a String HTML do elemento.
        * Data de criação: 28/03/2011
        * Histórico de Manutenções: (Data, Autor, Descrição da Alteração)
        *
        * @author Juliano S. De Cezaro <juliano_cezaro@uniritter.edu.br>
        * @version 1.0.0
        *
        * @return void
        */
        public function getHTML()
        {
			if (is_array($this->arrValues)) {
                foreach ($this->arrValues as $value => $label) {
					$this->append(new option("{".($this->params->value == $value ? "selected: selected, " : "")." value: $value, label: $label}"));
                }
            }
            return parent::getHTML();
        }
    }

